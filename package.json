{
  "name": "take-home-test",
  "version": "1.0.0",
  "repository": "git@github.com:inato/take-home-test.git",
  "author": "greenly",
  "license": "MIT",
  "main": "dist/index.js",
  "private": true,
  "engines": {
    "yarn": ">=1.7.0",
    "node": ">=12.x"
  },
  "scripts": {
    "lint": "eslint src",
    "start": "npx ts-node src/app.ts",
    "start:dev": "export NODE_ENV=dev && npx ts-node src/app.ts",
    "test": "jest --coverage --maxWorkers 5",
    "test-no-cache": "jest --coverage --maxWorkers 5 --no-cache",
    "test-last-commit": "jest --coverage --lastCommit --maxWorkers 5 --no-cache",
    "test-watch": "jest --coverage --watch --maxWorkers 5",
    "build": "tsc"
  },
  "devDependencies": {
    "@testdeck/jest": "^0.2.0",
    "@types/expect": "^24.3.0",
    "@types/jest": "^27.4.0",
    "@types/jest-when": "^2.7.4",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^27.4.7",
    "jest-when": "^3.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3",
    "regenerator-runtime": "^0.12.1",
    "ts-jest": "^27.1.3",
    "ts-test-decorators": "^0.0.6",
    "typescript": "^4.5.4"
  },
  "pre-commit": [
    "lint",
    "test",
    "build"
  ]
}
